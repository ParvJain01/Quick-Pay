<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.quick">
    <class name="ElectricityBill" table="electricitybill">
        <id name="serialNo">
            <generator class="native"/>
        </id>
        <property name="companyId"/>
      <property name="customerName"/>
        <property name="contactNo"/>
        <property name="address"/>
        <property name="ivrsNo"/>
        <property name="date"/>
        <property name="dueDate"/>
        <property name="meterNo"/>
        <property name="phases"/>
        <property name="unitConsumed"/>
        <property name="pricePerUnit"/>
        <property name="total"/>
        <property name="status"/>
    </class>



</hibernate-mapping>
